{
  "name": "wetube",
  "version": "1.0.0",
  "description": "Wetube Clone built using NodeJS, Express, Mongo and ES6",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cororom/wetube.git"
  },
  "scripts": {
    "start": "node build/init.js",
    "build": "npm run build:server && npm run build:assets",
    "build:server": "babel src -d build --ignore src/client",
    "build:assets": "webpack --mode=production",
    "dev:server": "nodemon",
    "dev:assets": "webpack --mode=development -w"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cororom/wetube/issues"
  },
  "homepage": "https://github.com/cororom/wetube#readme",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffmpeg/core": "^0.10.0",
    "@ffmpeg/ffmpeg": "^0.10.1",
    "@ffprobe-installer/ffprobe": "^1.4.1",
    "bcrypt": "^5.0.1",
    "connect-mongo": "^4.6.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.2",
    "fast-preview": "^1.0.9",
    "fluent-ffmpeg": "^2.1.2",
    "moment": "^2.29.3",
    "mongoose": "^6.2.8",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "node-fetch": "^2.6.7",
    "pug": "^3.0.2",
    "regenerator-runtime": "^0.13.9",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.8",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.4",
    "css-loader": "^6.7.1",
    "mini-css-extract-plugin": "^2.6.0",
    "nodemon": "^2.0.15",
    "sass": "^1.49.11",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2"
  }
}
